
#include <stdio.h>

int main(int argc, const char * argv[]) {
    
    
    
    for(int i = 1; i <= 100; i++){
        char divisable = 'B';

        if(i % 3 == 0){
            divisable = 'T';
        }if(i % 5 == 0){
            divisable = 'F';
        }if(i % 5 == 0 && i % 3 == 0){
            divisable = 'C';
        }
        
        switch(divisable){
            case 'T': printf("Fizz \n"); break;
            case 'F': printf("Buzz \n"); break;
            case 'C': printf("FizzBuzz \n"); break;
            case 'B': printf("%d \n", i);
                
        }
        
    }
    return 0;
} 
